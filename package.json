{
  "name": "telegram-bot",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "heroku": "node ./bin/www",
    "start": "env-cmd -f ./production.env node ./bin/www",
    "dev": "env-cmd -f ./local.env node ./bin/www",
    "debug": "env-cmd -f ./local.env node --inspect-brk ./bin/www",
    "watch": "tsc -w"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "env-cmd": "^10.0.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "sequelize": "^5.15.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/express": "^4.17.0",
    "@types/node": "^12.6.8",
    "@types/request": "^2.48.2",
    "@types/request-promise": "^4.1.44",
    "@types/validator": "^10.11.2",
    "typescript": "^3.7.2"
  }
}
